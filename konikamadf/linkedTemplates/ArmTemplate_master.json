					},
					"AzureSqlDimCustomerLS_connectionString": {
						"value": "[parameters('AzureSqlDimCustomerLS_connectionString')]"
					},
					"AzureSqlSalesLS_connectionString": {
						"value": "[parameters('AzureSqlSalesLS_connectionString')]"
					},
					"AzureSqlSampleDBLS_connectionString": {
						"value": "[parameters('AzureSqlSampleDBLS_connectionString')]"
					},
					"AzureSynapseAnalyticsLS_connectionString": {
						"value": "[parameters('AzureSynapseAnalyticsLS_connectionString')]"
					},
					"BlobSalesOutputLS_connectionString": {
						"value": "[parameters('BlobSalesOutputLS_connectionString')]"
					},
					"SampleDesLS_connectionString": {
						"value": "[parameters('SampleDesLS_connectionString')]"
					},
					"ADVSourceLS_connectionString": {
						"value": "[parameters('ADVSourceLS_connectionString')]"
					},
					"ADVSourceLS_password": {
						"value": "[parameters('ADVSourceLS_password')]"
					},
					"OnPremiseSQLServer_connectionString": {
						"value": "[parameters('OnPremiseSQLServer_connectionString')]"
					},
					"OnPremiseSQLServer_password": {
						"value": "[parameters('OnPremiseSQLServer_password')]"
					},
					"SalesDataLS_password": {
						"value": "[parameters('SalesDataLS_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ADVSourceLS_properties_typeProperties_userName": {
						"value": "[parameters('ADVSourceLS_properties_typeProperties_userName')]"
					},
					"OnPremiseSQLServer_properties_typeProperties_userName": {
						"value": "[parameters('OnPremiseSQLServer_properties_typeProperties_userName')]"
					},
					"SalesDataLS_properties_typeProperties_host": {
						"value": "[parameters('SalesDataLS_properties_typeProperties_host')]"
					},
					"SalesDataLS_properties_typeProperties_userId": {
						"value": "[parameters('SalesDataLS_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "konikamadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'konikamadf_ArmTemplate_0')]"
			]
		}
	]
}