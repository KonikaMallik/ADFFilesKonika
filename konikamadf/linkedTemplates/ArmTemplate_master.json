		"AzureSynapseAnalyticsLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalyticsLS'"
		},
		"BlobSalesOutputLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobSalesOutputLS'"
		},
		"OnPremiseSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremiseSQLServer'"
		},
		"OnPremiseSQLServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremiseSQLServer'"
		},
		"SalesDataLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SalesDataLS'"
		},
		"SampleDesLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SampleDesLS'"
		},
		"ADVSourceLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "malli"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://konikaadlsgen2.dfs.core.windows.net"
		},
		"OnPremiseSQLServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "malli"
		},
		"SalesDataLS_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\malli\\Downloads"
		},
		"SalesDataLS_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "malli"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "konikamadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
